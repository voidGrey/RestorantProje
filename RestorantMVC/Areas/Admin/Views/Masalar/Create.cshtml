@model Entites.Concrate.Masa

@{
    ViewData["Title"] = "Create";
}


<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="MasaID" class="control-label">Masa Numarası : </label>
                    <input asp-for="MasaID" class="form-control" />
                    <span asp-validation-for="MasaID" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Oluştur" class="btn btn-primary btn-icon-text" />
                    <a asp-action="Index" class="btn btn-secondary btn-fw mdi mdi-backburger">Listeye geri dön</a>
                </div>
            </form>
        </div>
    </div>
</div>
